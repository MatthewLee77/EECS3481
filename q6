package asymmetric;

import java.math.BigInteger;

public class q6
{
    public static void main(final String[] args) {
        final BigInteger e1 = new BigInteger("421");
        final BigInteger n1 = new BigInteger("");
        final BigInteger c1 = new BigInteger("");
        final BigInteger e2 = new BigInteger("4294967297");
        final BigInteger n2 = new BigInteger("");
        final BigInteger c2 = new BigInteger("");
        final BigInteger gcd = n1.gcd(n2);
        final BigInteger m = c1.multiply(c2);
        final BigInteger pt = m.mod(n1);
        System.out.println(pt);
        System.out.println(new String(pt.toByteArray()));
    }
}
